	<keywords>
		<!-- keywords for indexing -->
		<keyword>APEXMIC</keyword>
		<keyword>Device Support</keyword>
		<keyword>Device Family Package APEX Microelectronics</keyword>
		<keyword>APM32F00</keyword>
